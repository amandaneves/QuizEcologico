<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quiz.br.quizecologico" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Inicial"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:screenOrientation="portrait"
            android:label="Quiz Ecológico">
        </activity>
        <activity
            android:name=".Nivel"
            android:screenOrientation="portrait"
            android:label="Níveis">
        </activity>
        <activity
            android:name=".Info"
            android:screenOrientation="portrait"
            android:label="Sobre o Aplicativo">
        </activity>
        <activity
            android:name=".Perguntas_fund_ini"
            android:screenOrientation="portrait"
            android:label="Anos iniciais (Fundamental)">
        </activity>
        <activity
            android:name=".Perguntas_fund_fin"
            android:screenOrientation="portrait"
            android:label="Anos finais (Fundamental)">
        </activity>
        <activity
            android:name=".Perguntas_medio"
            android:screenOrientation="portrait"
            android:label="Ensino Médio">
        </activity>
        <activity
            android:name=".Final_iniciais"
            android:screenOrientation="portrait"
            android:label="Anos iniciais (Fundamental)">
        </activity>
    </application>
</manifest>
